 <style>
* {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
body {
  font-family: Helvetica, Arial, sans-serif;
  background-color: #f8f9fa;
}
heading {
  font-size: 14px;
  display: block;
  text-align: center;
  padding: 5px;
  margin-bottom: 10px;
  background-color: rgb(138, 159, 173);
  color: #fff;   
  height: 30px;
}
h2 {
  font-size: 12px;
  color: rgba(0, 0, 0, 0.5);
  margin-bottom: 10px;
}
a {
  text-decoration: none;
}
hr {
  color: rgba(0, 0, 0, 0.15);
  border: dotted 1px rgba(0, 0, 0, 0.15);
  margin: 5px 0;
}
time {
  color: rgba(0, 0, 0, 0.5);
  font-size: 12px;
}
time,
h2 {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
#board {
  display: flex;
  padding:10px;  
}
.column {
  width: 20%;
  border-right: solid 1px  #d9d9d9;
  }
.column:first-child {
  border-left: solid 1px  #d9d9d9;
  }

.columnTitle {
  font-size: 16px;
  text-align: center;
  padding: 4px;
  background-color: #428bca;
}

.job {
  background: #fff;
  position: relative;
  padding: 30px 10px 10px 10px;
  margin-bottom: 10px;
  margin-left: 10px;
  margin-right: 10px;
  border-radius: 5px;
  box-shadow: 3px 3px 3px rgba(0, 0, 0, 0.1);
  overflow: hidden;
}
.column.done .job {
  opacity: 0.75;
  transition: all 0.5s;
}
.column.done .job:hover {
  opacity: 1;
}
.job:hover {
  cursor: move;
}
.top {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 20px;
  border-bottom: solid 1px rgba(0, 0, 0, 0.1);
  padding: 5px;
}

.colorBlue {
  background:#428bca;
}

.colorGray {
  background:#ccc;
}

.colorGreen {
  background:green;
}

.colorRed{
  background:red;
}

.colorOrange {
  background:orange;
}

.close {
  background: black;
  border-radius: 10px;
  width: 10px;
  height: 10px;
  position: absolute;
  top: 5px;
  right: 5px;
  border: solid 1px rgba(0, 0, 0, 0.25);
  border-style: inset;
  opacity: 0.35;
  transition: opacity 0.3s;
}
.close:hover {
  opacity: 1;
}
  </style>
</head>
<script>
    
  $(document).ready(draggableInit);
  
    function  draggableInit(){

      var sourceColumnId;

      $('[draggable=true]').bind('dragstart', function (event) {
      
        sourceColumnId = $(this).parent().attr('id');
        event.originalEvent.dataTransfer.setData("text/plain", event.target.getAttribute('id'));
        console.log('dragstart sourceId: ' +  sourceColumnId + " - itemId: " + event.target.getAttribute('id'))
      });

      
    $('.column').bind('dragover', function (event) {
        event.preventDefault();        
    });

    $('.column').bind('drop', function (event) {
            
      var targetColumn = $(this);
      var targetColumnId = targetColumn.attr('id');
      
        if (sourceColumnId != targetColumnId) {
            var elementId = event.originalEvent.dataTransfer.getData("text/plain");
            var element = document.getElementById(elementId);
            
            console.log($("#" + targetColumnId + " div:first-child"));
            
            $(targetColumn.children()[0]).after(element);
            
        }
        event.preventDefault();
    });

  }

</script>
