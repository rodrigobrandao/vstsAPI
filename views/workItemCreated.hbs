<form>

<div class="form-group row">
    <label class="col-sm-1 col-form-label">Protocolo</label>
    <div class="col-sm-10">
      <a href="https://atentoalm.visualstudio.com/AtentoBR/_workitems/edit/{{{WorkItem.Id}}}">{{{WorkItem.Id}}}</a>
    </div>
</div>

<div class="form-group row">
    <label class="col-sm-1 col-form-label">Assunto</label>
    <div class="col-sm-10">
      <input type="text" readonly class="form-control-plaintext" value="{{{WorkItem.Title}}}">
    </div>
</div>

<div class="form-group row">
    <label class="col-sm-1 col-form-label">E-mail</label>
    <div class="col-sm-10">
      <input type="text" readonly class="form-control-plaintext" value="{{{WorkItem.AssignTo}}}">
    </div>
</div>

<div class="form-group row">
    <label class="col-sm-1 col-form-label">Conteudo</label>
    <div class="col-sm-10">
      <input type="text" readonly class="form-control-plaintext" value="{{{WorkItem.Description}}}">
    </div>
</div>

<div class="form-group row">
  Acompanhe em: &nbsp;<a href="https://atentoalm.visualstudio.com/AtentoBR/_boards/board/t/Estudo/Requirements" target="_blank">https://atentoalm.visualstudio.com/AtentoBR/_boards/board/t/Estudo/Requirements</a>
</div>

</form>